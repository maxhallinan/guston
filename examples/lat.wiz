; lat? evaluates to true if xs is a list of atoms, otherwise false
; from The Little Schemer by Daniel P. Friedman and Matthias Felleisen
(define lat?
  (lambda
    (xs)
    (cond
      ((eq? xs (quote ())) (quote t))
      ((atom? (car xs)) (lat? (cdr xs)))
      ((quote t) (quote false)))))

(lat? (quote (a b c)))
