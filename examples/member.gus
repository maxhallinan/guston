; member? evaluates to true if the atom x is in the list xs, otherwise false
; from The Little Schemer by Daniel P. Friedman and Matthias Felleisen
(define member?
  (lambda (x xs)
    (cond
      ((eq? xs (quote ())) (quote ()))
      ((quote t) (cond
        ((eq? x (car xs)) (quote t))
        ((quote t) (member? x (cdr xs))))))))

(member? (quote c) (quote (a b c)))
